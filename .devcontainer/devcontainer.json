{
  "postCreateCommand": "npm i && npm i --prefix=functions/ssr",
  "waitFor": "postCreateCommand",
  "portsAttributes": {
    "4321": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [
    4321
  ],
  "postAttachCommand": {
    "Server": "npm run dev"
  },
  "customizations": {
    "codespaces": {
      "openFiles": [
        "functions/ssr/src/main/Home.astro"
      ]
    },
    "vscode": {
      "extensions": [
        "astro-build.astro-vscode",
        "vue.volar",
        "bradlc.vscode-tailwindcss"
      ]
    }
  }
}
