---
import { filterMainCategories } from '@@sf/composables/use-shop-header';
import ShopFooter from '~/components/ShopFooter.vue';

const { routeContext } = Astro.locals;
const layoutContent = await routeContext.getContent('layout');
const {
  service_links: serviceLinks,
  footer: {
    stamps,
    payment_methods: paymentMethods,
  },
} = layoutContent;
---

<ShopFooter {serviceLinks} {stamps} {paymentMethods} />
