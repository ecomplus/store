---
import SfBase from '@@sf/layouts/Base.astro';
import InlineScripts from '~/scripts/InlineScripts.astro';
import '~/assets/style.css';
import 'uno.css';

export interface Props {
  title?: string;
}

const { title } = Astro.props;
---

<SfBase title={title}>
  <InlineScripts slot="before-head-end" />
  <slot />
</SfBase>
