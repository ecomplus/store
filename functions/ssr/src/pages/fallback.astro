---
import ViewFallback from '@cloudcommerce/storefront/src/lib/views/fallback.astro';
import loadPageContext, { PageContext } from '@cloudcommerce/storefront/src/lib/ssr-context';

let pageContext: PageContext;
try {
  pageContext = await loadPageContext(Astro);
} catch (err) {
  return err.astroResponse;
}
---

<ViewFallback pageContext={pageContext} />
